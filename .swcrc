{
    "jsc": {
        "target": "es2020",
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": false,
            "decoratorMetadata": false,
            "useDefineForClassFields": false,
            "hidden": {
                "jest": false
            }
        },
        "keepClassNames": false
    },
    "module": {
        "type": "es6"
    },
    "sourceMaps": false,
    "inlineSourcesContent": false
}